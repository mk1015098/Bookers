<h1>books edit</h1>
<h2>Editing Book</h2>

<%#= form_with model: @book do |f| %>
<%= form_with model: @book, url: update_book_path(@book.id), method: :patch do |f| %>
  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Opinion</h4>
  <%= f.text_area :body %>

  <%= f.submit 'Update Book' %>

<% end %>
